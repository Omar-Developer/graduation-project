<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Password">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        android:padding="50dp"
        android:layout_gravity="center"
        android:id="@+id/tip">

        <include layout="@layout/help3"
                 android:visibility="invisible"
                 android:id="@+id/help3"/>
        <include layout="@layout/help2"
                 android:visibility="invisible"
                 android:id="@+id/help2"/>
        <include layout="@layout/help1"
                 android:visibility="invisible"
                 android:id="@+id/help1"/>
        <include layout="@layout/help"
                 android:id="@+id/help"/>
    </RelativeLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        <Button
            android:id="@+id/submitShape"
            android:layout_width="150dp"
            android:layout_height="43dp"
            android:layout_marginStart="48dp"
            android:layout_marginLeft="48dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/passbutton"
            android:fontFamily="@font/regular"
            android:text="Submit Shape"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/bluebutton"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/deleteShape"
            android:layout_width="150dp"
            android:layout_height="43dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/deletebutton"
            android:fontFamily="@font/regular"
            android:text="Delete Shape"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/yellowbutton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.285"
            app:layout_constraintStart_toEndOf="@+id/submitShape" />

        <Button
            android:id="@+id/done"
            android:layout_width="160dp"
            android:layout_height="43dp"
            android:background="@drawable/button"
            android:fontFamily="@font/regular"
            android:text="Done!"
            android:textAllCaps="false"
            android:textColor="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.521"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.955" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/regular"
            android:text="Created By KSU Computer Science Students."
            android:textColor="@color/darkBlue"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.021"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.998" />


        <ImageView
            android:id="@+id/ksuLogo"
            android:layout_width="44dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.956"
            app:layout_constraintStart_toEndOf="@+id/back"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.023"
            app:srcCompat="@drawable/question" />

        <ImageView
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toTopOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.091"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.04"
            app:srcCompat="@drawable/arrow" />

        <ImageButton
            android:id="@+id/rose"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/cloud"
            app:layout_constraintStart_toEndOf="@+id/smile"
            app:srcCompat="@drawable/rose" />

        <ImageButton
            android:id="@+id/finger"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="64dp"
            android:layout_marginLeft="64dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/smile"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/finger" />

        <ImageButton
            android:id="@+id/smile"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="64dp"
            android:layout_marginLeft="64dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/tree"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/smile" />

        <ImageButton
            android:id="@+id/car"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/mosque"
            app:layout_constraintStart_toEndOf="@+id/rose"
            app:srcCompat="@drawable/car" />

        <ImageButton
            android:id="@+id/moon"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/fire"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/car"
            app:srcCompat="@drawable/moon" />

        <ImageButton
            android:id="@+id/tree"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="64dp"
            android:layout_marginLeft="64dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/done"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/tree" />

        <ImageButton
            android:id="@+id/bag"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/car"
            app:layout_constraintStart_toEndOf="@+id/heart"
            app:srcCompat="@drawable/bag" />

        <ImageButton
            android:id="@+id/apple"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/moon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/bag"
            app:srcCompat="@drawable/apple" />

        <ImageButton
            android:id="@+id/mosque"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/done"
            app:layout_constraintStart_toEndOf="@+id/cloud"
            app:srcCompat="@drawable/mosque" />

        <ImageButton
            android:id="@+id/cloud"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/done"
            app:layout_constraintStart_toEndOf="@+id/tree"
            app:srcCompat="@drawable/cloud" />

        <ImageButton
            android:id="@+id/fire"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            app:layout_constraintBottom_toTopOf="@+id/done"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/mosque"
            app:srcCompat="@drawable/fire" />

        <ImageButton
            android:id="@+id/heart"
            android:layout_width="61dp"
            android:layout_height="61dp"
            android:layout_marginStart="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/symbolsbutton"
            android:contentDescription="TODO"
            app:layout_constraintBottom_toTopOf="@+id/rose"
            app:layout_constraintStart_toEndOf="@+id/finger"
            app:srcCompat="@drawable/heart" />

        <Button
            android:id="@+id/bluebutton"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_marginStart="72dp"
            android:layout_marginLeft="72dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/bluebutton"
            android:text=""
            app:layout_constraintBottom_toTopOf="@+id/finger"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/blackbutton"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_marginStart="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/blackbutton"
            android:text=""
            app:layout_constraintBottom_toTopOf="@+id/heart"
            app:layout_constraintStart_toEndOf="@+id/bluebutton" />

        <Button
            android:id="@+id/yellowbutton"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_marginStart="32dp"
            android:layout_marginLeft="32dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/yellowbutton"
            android:text=""
            app:layout_constraintBottom_toTopOf="@+id/bag"
            app:layout_constraintStart_toEndOf="@+id/blackbutton" />

        <Button
            android:id="@+id/redbutton"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/rebutton"
            android:text=""
            app:layout_constraintBottom_toTopOf="@+id/apple"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.313"
            app:layout_constraintStart_toEndOf="@+id/yellowbutton" />

        <ImageButton
            android:id="@+id/rotateLeft"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="88dp"
            android:layout_marginLeft="88dp"
            android:layout_marginBottom="40dp"
            android:background="@null"
            app:layout_constraintBottom_toTopOf="@+id/submitShape"
            app:layout_constraintEnd_toStartOf="@+id/preview"
            app:layout_constraintHorizontal_bias="0.391"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/back"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/rotateleft" />

        <ImageButton
            android:id="@+id/rotateRight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="84dp"
            android:layout_marginRight="84dp"
            android:layout_marginBottom="40dp"
            android:background="@null"
            app:layout_constraintBottom_toTopOf="@+id/deleteShape"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.529"
            app:layout_constraintStart_toEndOf="@+id/preview"
            app:layout_constraintTop_toBottomOf="@+id/ksuLogo"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/rotateright" />

        <ImageView
            android:id="@+id/preview"
            android:layout_width="74dp"
            android:layout_height="74dp"
            android:background="@drawable/symbolsbutton"
            android:padding="15dp"
            app:layout_constraintBottom_toTopOf="@+id/submitShape"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ksuLogo"
            app:layout_constraintVertical_bias="0.812"
            app:srcCompat="@drawable/finger" />

        <LinearLayout
            android:id="@+id/dots"
            android:layout_width="300dp"
            android:layout_height="25dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintBottom_toTopOf="@+id/preview"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ksuLogo"></LinearLayout>

        <ImageButton
            android:id="@+id/eye"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@+id/preview"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/dots"
            app:layout_constraintTop_toBottomOf="@+id/ksuLogo"
            app:layout_constraintVertical_bias="0.493"
            app:srcCompat="@drawable/eye" />

        <ImageButton
            android:id="@+id/eye_off"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"
            android:visibility="visible"
            app:layout_constraintBottom_toTopOf="@+id/preview"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/dots"
            app:layout_constraintTop_toBottomOf="@+id/ksuLogo"
            app:layout_constraintVertical_bias="0.493"
            app:srcCompat="@drawable/eyeoff" />



    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>